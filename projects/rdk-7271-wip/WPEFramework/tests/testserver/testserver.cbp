<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="testserver" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="../../artifacts/debug/testserver" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".../../artifacts/debug/" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-I../../" />
					<Add option="-D__DEBUG__" />
				</Compiler>
				<Linker>
					<Add directory="../../artifacts/debug/" />
				</Linker>
				<MakeCommands>
					<Build command="$make -f $makefile" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile" />
					<SilentBuild command="$make -f $makefile &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="../../artifacts/release/testserver" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../artifacts/release/" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../../artifacts/release/" />
				</Linker>
				<MakeCommands>
					<Build command="$make -f $makefile TYPE=Release" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile TYPE=Release clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile TYPE=Release" />
					<SilentBuild command="$make -f $makefile TYPE=Release &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="../../../WPEFramework" />
		</Compiler>
		<Linker>
			<Add library="devices" />
			<Add library="websocket" />
			<Add library="generics" />
			<Add library="dl" />
		</Linker>
		<Unit filename="DataContainer.cpp" />
		<Unit filename="DataContainer.h" />
		<Unit filename="EchoProtocol.h" />
		<Unit filename="testserver.cpp" />
		<Extensions>
			<code_completion>
				<search_path add="../../" />
			</code_completion>
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
