set (autostart ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_AUTOSTART})

if(WPEFRAMEWORK_PLUGIN_WEBSERVER)
set (preconditions Graphics WebSource)
else()
set (preconditions Graphics)
endif(WPEFRAMEWORK_PLUGIN_WEBSERVER)

map()
    kv(url ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_STARTURL})
    kv(useragent ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_USERAGENT})
    kv(injectedbundle "libWPEInjectedBundle${CMAKE_SHARED_LIBRARY_SUFFIX}")
    kv(transparent ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_TRANSPARENT})
    kv(compositor "noaa")
    kv(inspector "0.0.0.0:9998")
    kv(fps true)
    kv(cursor false)
    kv(touch false)
    kv(msebuffers "audio:2m,video:15m,text:1m")
    kv(memoryprofile ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_MEMORYPROFILE})
    kv(memorypressure ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_MEMORYPRESSURE})
    kv(mediadiskcache ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_MEDIADISKCACHE})
    kv(diskcache ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_DISKCACHE})
    kv(xhrcache ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_XHRCACHE})
    kv(webgl ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_WEBGL})
    if(WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_CLIENTIDENTIFIER)
        kv(clientidentifier ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_CLIENTIDENTIFIER})
    endif()
    if(WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_LOCALSTORAGE)
        kv(localstorage ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_LOCALSTORAGE})
    endif()
    if(WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_COOKIESTORAGE)
        kv(cookiestorage ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_COOKIESTORAGE})
    endif()
    if(WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_WINDOWCLOSE)
        kv(windowclose ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_WINDOWCLOSE})
    endif()
end()
ans(configuration)
